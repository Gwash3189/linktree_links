{
  "extends": [
    "airbnb-typescript/base",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "project": "tsconfig.json"
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  },
  "rules": {
    "semi": "off",
    "@typescript-eslint/semi": ["off"],
    "import/prefer-default-export": 0,
    "no-shadow": 0,
    "no-param-reassign": 0
  }
}
